fib :: (n: int) -> int {
    a := 0;
    b := 0;
    c := 1;
    counter := 1;
    loop {
        counter = counter + 1;
        if (counter > n) {
            break;
        }
        a = b;
        b = c;
        c = a + b;
    }
    return c;
}

main :: () -> int {
    result := fib(30);
    iprint(result);
    return 0;
}
